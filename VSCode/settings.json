{
    "workbench.startupEditor": "none",
    "editor.minimap.enabled": false,
    "[razor]": {},
    "java.errors.incompleteClasspath.severity": "ignore",
    "workbench.statusBar.feedback.visible": false,
    "breadcrumbs.enabled": true,
    "editor.formatOnType": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "gitlens.hovers.enabled": false,
    "telemetry.telemetryLevel": "off",
    "editor.rulers": [100],
    "workbench.sideBar.location": "left",
    "git.enableCommitSigning": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.completion.overwrite": false,
    "java.format.settings.url": "https://raw.githubusercontent.com/fgonzal/.dotfiles/main/VSCode/java-formatter.xml",
    "editor.fontFamily": "JetBrains Mono, Cascadia Code, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.tabCompletion": "on",
    "gitlens.statusBar.enabled": false,
    "editor.lineHeight": 24,
    "workbench.iconTheme": "material-icon-theme",
    "java.codeGeneration.useBlocks": true,
    "java.semanticHighlighting.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "java.refactor.renameFromFileExplorer": "autoApply",
    "sonarlint.rules": {
        "java:S1172": {
            "level": "off"
        }
    },
    "java.project.importOnFirstTimeStartup": "automatic",
    "editor.formatOnSaveMode": "modifications",
    "java.configuration.updateBuildConfiguration": "automatic",
    // "workbench.colorTheme": "Plastic Legacy v2 - deprioritised punctuation",
    "java.import.gradle.home": "",
    // "java.import.gradle.version": "6.8.2",
    "java.import.gradle.offline.enabled": true,
    // "java.import.gradle.java.home": "/Users/fgonzalez/.sdkman/candidates/java/current",
    "java.import.gradle.java.home": "/Library/Java/JavaVirtualMachines/adoptopenjdk-11.jdk/Contents/Home",
    "java.jdt.ls.java.home": "/Library/Java/JavaVirtualMachines/adoptopenjdk-11.jdk/Contents/Home",
    "workbench.editor.wrapTabs": true,
    "terminal.explorerKind": "external",
    "terminal.external.osxExec": "iTerm.app",
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#3a355396",
        "statusBar.background": "#551d5c",
        "statusBar.foreground": "#bba4bd",
        "editor.background": "#1a1a2c",
        "sideBar.background": "#1D1D31",
        "activityBar.background": "#1D142B"
    },
    "redhat.telemetry.enabled": false,
    "java.eclipse.downloadSources": true,
    "java.implementationsCodeLens.enabled": true,
    "java.maven.downloadSources": true,
    "java.sources.organizeImports.starThreshold": 10,
    "http.proxySupport": "off",
    "editor.scrollbar.scrollByPage": true,
    "workbench.productIconTheme": "icons-carbon",
    "java.import.exclusions": [
        "**/bin/**",
        "**/build/**",
        "**/node_modules/**",
        "**/.metadata/**",
        "**/archetype-resources/**",
        "**/META-INF/maven/**"
    ],
    "tabnine.experimentalAutoImports": true,
    "editor.wordWrapColumn": 100,
    "java.format.settings.profile": "JavaConventions"
}
